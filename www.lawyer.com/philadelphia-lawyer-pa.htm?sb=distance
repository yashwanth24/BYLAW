                        <script>
function setMarkers(map,locations){
 var  i;
 for (i = 0; i < locations.length; i++)
 {
     var loan = locations[i][0];
     var lat = locations[i][1];
     var long = locations[i][2];
     var add =  locations[i][3];
     var contentString = locations[i][4];
     latlngset = new google.maps.LatLng(lat, long);
     var marker = new google.maps.Marker({
         map: map,
         title: loan,
         position: latlngset,
         animation: google.maps.Animation.DROP,
         label: add
     });
     
     var infowindow = new google.maps.InfoWindow({
         content: contentString
     });
     (function(infowindow) {
         google.maps.event.addListener(marker, 'mouseover', function() {
             infowindow.open(map, this);
         });
         google.maps.event.addListener(marker, 'mouseout', function() {
               infowindow.close();
           });
       })(infowindow);
   }
}
function setboundery(map,locations){
 var markers=[], i;
 for (i = 0; i < locations.length; i++)
 {
     var loan = locations[i][0];
     var lat = locations[i][1];
     var long = locations[i][2];
     var add =  locations[i][3];
     latlngset = new google.maps.LatLng(lat, long);
     var marker = new google.maps.Marker({
         map: map,
         title: loan ,
         position: latlngset,
         animation: google.maps.Animation.DROP,
         label: add
     });
     markers.push(marker);
 }
 var bounds = new google.maps.LatLngBounds();
 for(i=0;i<markers.length;i++) {
     bounds.extend(markers[i].getPosition());
 }
 //center the map to the geometric center of all markers
 map.setCenter(bounds.getCenter());
 map.fitBounds(bounds);
 //remove one zoom level to ensure no marker is on the edge.
 map.setZoom(map.getZoom()-1);
}

function initMap() {
var map = new google.maps.Map(document.getElementById('map'), {
   zoom: 13,
});
setboundery(map,locations);
setMarkers(map,locations);
}    
</script>                </div> 

                <div class="panel panel-default side-widget"><div class="panel-heading"><h3 class="panel-title">TIPS</h3></div><div class="panel-body"><p>Lawyer.com makes finding Philadelphia Lawyers fast and easy. Refine your search for Philadelphia lawyers by legal practice area. Find a quality and affordable lawyer by reviewing detail profiles for Philadelphia attorneys. Many Lawyer.com featured Lawyers in Pennsylvania will offer free legal consultations and all have Pennsylvania law expertise.</p></div></div><div id="legalcase_side" class="panel panel-default side-widget">
                         <div class="panel-heading">
                            <h3 class="panel-title">SAMPLE LEGAL CASES</h3>
                         </div>
                         <div class="panel-body"><div class="panel panel-default panel-sub">
