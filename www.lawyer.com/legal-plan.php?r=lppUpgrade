	<script>
	//plan option radio buttons

	var divSelectFamInfo = document.getElementById("lppPlanFamInfo");
	var divSelectSingleInfo = document.getElementById("lppPlanSingleInfo");

	var spanLPPPriceTop = document.getElementById("LPPPriceTop");
	var txtLPPPriceTop = document.createTextNode("Family Legal Protection Plan only $14.99 per month subscription");
	spanLPPPriceTop.appendChild(txtLPPPriceTop);

	var spanLppPrice1 = document.getElementById("lppPrice1");
	var txtLPPPrice1 = document.createTextNode("$14.99");
	spanLppPrice1.appendChild(txtLPPPrice1);

	$('input[name="lppPlanId"]').on('change', function () {
	    if(this.value === "legalplansingle"){
	        var txtLPPPriceTop = document.createTextNode("Legal Protection Plan only $9.99 per month subscription");
	        var txtLPPPrice1 = document.createTextNode("$9.99");
	        var txtLPPSelectInfo = document.createTextNode("Legal Protection Plan benefits are available to the member only.");

	        divSelectFamInfo.textContent = '';
		    divSelectSingleInfo.textContent = '';
		    divSelectSingleInfo.appendChild(txtLPPSelectInfo);
	    } else {
	        var txtLPPPriceTop = document.createTextNode("Family Legal Protection Plan only $14.99 per month subscription");
	        var txtLPPPrice1 = document.createTextNode("$14.99");
	        var txtLPPSelectInfo = document.createTextNode("Family Legal Protection Plan benefits are available to the member + spouse/domestic partner and household children under the age of 24.");

	        divSelectSingleInfo.textContent = '';
		    divSelectFamInfo.textContent = '';
		    divSelectFamInfo.appendChild(txtLPPSelectInfo);
	    }

	    spanLPPPriceTop.textContent = '';
	    spanLPPPriceTop.appendChild(txtLPPPriceTop);

	    spanLppPrice1.textContent = '';
	    spanLppPrice1.appendChild(txtLPPPrice1);
	});

	//set plan option radio button checked
	function setSinglePlanOptionValue(){
		$("#lppPlanSingle").prop("checked", true);
		var txtLPPPriceTop = document.createTextNode("Legal Protection Plan only $9.99 per month subscription");
        var txtLPPPrice1 = document.createTextNode("$9.99");
        var txtLPPSelectInfo = document.createTextNode("Legal Protection Plan benefits are available to the member only.");

        divSelectFamInfo.textContent = '';
        divSelectSingleInfo.textContent = '';
	    divSelectSingleInfo.appendChild(txtLPPSelectInfo);

        spanLPPPriceTop.textContent = '';
	    spanLPPPriceTop.appendChild(txtLPPPriceTop);

	    spanLppPrice1.textContent = '';
	    spanLppPrice1.appendChild(txtLPPPrice1);
	}
	function setFamPlanOptionValue(){
		$("#lppPlanFamily").prop("checked", true);
		var txtLPPPriceTop = document.createTextNode("Family Legal Protection Plan only $14.99 per month subscription");
	    var txtLPPPrice1 = document.createTextNode("$14.99");
	    var txtLPPSelectInfo = document.createTextNode("Family Legal Protection Plan benefits are available to the member + spouse/domestic partner and household children under the age of 24.");

        divSelectSingleInfo.textContent = '';
        divSelectFamInfo.textContent = '';
	    divSelectFamInfo.appendChild(txtLPPSelectInfo);

        spanLPPPriceTop.textContent = '';
	    spanLPPPriceTop.appendChild(txtLPPPriceTop);

	    spanLppPrice1.textContent = '';
	    spanLppPrice1.appendChild(txtLPPPrice1);
	}


	// LPP join modal
	var modal = document.getElementById('lppJoinModal');
	var modalThanks = document.getElementById('lppJoinModalThanks');
	var modalError = document.getElementById('lppJoinModalError');
	var modalMoreInfo = document.getElementById('lppModalMoreInfo');
	var modalFree = document.getElementById('lppJoinModalFree');

	// LPP join now button to open modal
	var btnFam = document.getElementById("btnLppJoinFamily");
	var btnSingle = document.getElementById("btnLppJoinSingle");
	var btn1 = document.getElementById("txtLppJoin");
	var btn2 = document.getElementById("txtLppJoinMoreInfo");
	var btnFree = document.getElementById("btnLppJoinFree");

	//btn3
	var btnInfoFam = document.getElementById("btnLppJoinInfoFam");
	var btnInfoSingle = document.getElementById("btnLppJoinInfoSingle");


	//with random hero images instead of rotator - 
	//btn ID will always be the same as only one hero image/btn will be loaded at a time
	var btnInfo1 = document.getElementById("btnLppMoreInfo1");
	var btnInfoText = document.getElementById("txtLppMoreInfo");

	// <span> element that closes the modal
	var span = document.getElementsByClassName("lpp-modal-close")[0];
	var spanThanks = document.getElementsByClassName("lpp-modalThanks-close")[0];
	var spanError = document.getElementsByClassName("lpp-modalError-close")[0];
	var spanMoreInfo = document.getElementsByClassName("lpp-modalMoreInfo-close")[0];
	var spanFree = document.getElementsByClassName("lpp-modalFree-close")[0];

	// user clicks button, open modal 
	btnFam.onclick = function() {
	    modal.style.display = "block";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    setFamPlanOptionValue();
	    ga('send', 'event', 'LPP', 'Join Now', 'Main-JN-Button-Direct-2');	}
	btnSingle.onclick = function() {
	    modal.style.display = "block";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    setSinglePlanOptionValue();
	    ga('send', 'event', 'LPP', 'Join Now', 'Main-JN-Button-Direct-2');	}

	//Join Now text buttons - default to Family Plan selectec
	btn1.onclick = function() {
	    modal.style.display = "block";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    setFamPlanOptionValue();
	    ga('send', 'event', 'LPP', 'Join Now', 'Main-JN-Text-Direct-2');	}
	btn2.onclick = function() {
	    modal.style.display = "block";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    setFamPlanOptionValue();
	    ga('send', 'event', 'LPP', 'Join Now', 'Benefits-JN-Text-Direct-2');	}




	btnInfoSingle.onclick = function() {
	    modal.style.display = "block";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    setSinglePlanOptionValue();
	    ga('send', 'event', 'LPP', 'Join Now', 'Benefits-JN-Button-Direct-2');	}
	btnInfoFam.onclick = function() {
	    modal.style.display = "block";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    setFamPlanOptionValue();
	    ga('send', 'event', 'LPP', 'Join Now', 'Benefits-JN-Button-Direct-2');	}



	btnInfo1.onclick = function() {
	    modalMoreInfo.style.display = "block";
	    ga('send', 'event', 'LPP', 'Learn More', 'Main-LM-Button-Direct-2');	}



	btnInfoText.onclick = function() {
	    modalMoreInfo.style.display = "block";
	    ga('send', 'event', 'LPP', 'Learn More', 'Main-LM-Text-Direct-2');	}

	function showThanksModal() {
	    modal.style.display = "none";
	    modalThanks.style.display = "block";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	}

	function showFreeModal() {
	    modal.style.display = "none";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "block";
	}

	function showErrorModal() {
	    modal.style.display = "none";
	    modalThanks.style.display = "none";
	    modalError.style.display = "block";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	}

	//for rep sent link
	function showJoinModal() {
	    modal.style.display = "block";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    setFamPlanOptionValue();
	}


	// user clicks on <span> (x), close modal
	span.onclick = function() {
	    modal.style.display = "none";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	}

	spanMoreInfo.onclick = function() {
	    modal.style.display = "none";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	}

	spanThanks.onclick = function() {
	    modal.style.display = "none";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    document.location.replace('/legal-plan.php');
	}

	spanError.onclick = function() {
	    modal.style.display = "none";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    document.location.replace('/legal-plan.php');
	}

	spanFree.onclick = function() {
	    modal.style.display = "none";
	    modalThanks.style.display = "none";
	    modalError.style.display = "none";
	    modalMoreInfo.style.display = "none";
	    modalFree.style.display = "none";
	    document.location.replace('/legal-plan.php');
	}

	// user clicks anywhere outside modal, close it
	window.onclick = function(event) {
	    if (event.target == modal) {
	        modal.style.display = "none";
	    }
	    if (event.target == modalMoreInfo) {
	        modalMoreInfo.style.display = "none";
	    }
	    if (event.target == modalThanks) {
	        modalThanks.style.display = "none";
	        document.location.replace('/legal-plan.php');
	    }
	    if (event.target == modalError) {
	        modalError.style.display = "none";
	        document.location.replace('/legal-plan.php');
	    }
	    if (event.target == modalFree) {
	        modalFree.style.display = "none";
	        document.location.replace('/legal-plan.php');
	    }
	}

	</script>

