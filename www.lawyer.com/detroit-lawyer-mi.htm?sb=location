<script>
    function gObj(obj){var theObj;if(document.all){if(typeof obj=="string"){return document.all(obj);}else{return obj.style;}}if(document.getElementById){if(typeof obj=="string"){return document.getElementById(obj);}else{return obj.style;}}return null;}
    function filterVals(inVal){
        var pdf = "detroit-lawyer-mi.htm?sb=location";
        if (inVal==1){
            pdf +="&al=1";
        }else{
            if (gObj("genderm").checked) pdf +="&gm=1";
            if (gObj("genderf").checked) pdf +="&gf=1";
            if (gObj("freeconsultation").checked) pdf +="&fc=1";
            if (gObj("fsuspended").checked) pdf +="&ss=1";
        }
        window.location = pdf;
    }
</script>


